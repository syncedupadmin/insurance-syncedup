{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },

    { "source": "/admin", "destination": "/_admin/index.html" },
    { "source": "/admin/", "destination": "/_admin/index.html" },
    { "source": "/manager", "destination": "/_manager/index.html" },
    { "source": "/manager/", "destination": "/_manager/index.html" },
    { "source": "/agent", "destination": "/_agent/index.html" },
    { "source": "/agent/", "destination": "/_agent/index.html" },
    { "source": "/customer-service", "destination": "/_customer-service/index.html" },
    { "source": "/customer-service/", "destination": "/_customer-service/index.html" },
    { "source": "/super-admin", "destination": "/_super-admin/index.html" },
    { "source": "/super-admin/", "destination": "/_super-admin/index.html" },
    { "source": "/leaderboard", "destination": "/_leaderboard/index.html" },
    { "source": "/leaderboard/", "destination": "/_leaderboard/index.html" },

    { "source": "/admin/:page", "destination": "/_admin/:page.html" },
    { "source": "/manager/:page", "destination": "/_manager/:page.html" },
    { "source": "/agent/:page", "destination": "/_agent/:page.html" },
    { "source": "/customer-service/:page", "destination": "/_customer-service/:page.html" },
    { "source": "/super-admin/:page", "destination": "/_super-admin/:page.html" },
    { "source": "/leaderboard/:page", "destination": "/_leaderboard/:page.html" },

    { "source": "/admin/(.*)", "destination": "/_admin/$1" },
    { "source": "/manager/(.*)", "destination": "/_manager/$1" },
    { "source": "/agent/(.*)", "destination": "/_agent/$1" },
    { "source": "/customer-service/(.*)", "destination": "/_customer-service/$1" },
    { "source": "/super-admin/(.*)", "destination": "/_super-admin/$1" },
    { "source": "/leaderboard/(.*)", "destination": "/_leaderboard/$1" },

    { "source": "/", "destination": "/index.html" },
    { "source": "/login", "destination": "/login.html" },
    { "source": "/pricing", "destination": "/pricing.html" },
    { "source": "/trial", "destination": "/signup.html" },
    { "source": "/signup", "destination": "/signup.html" },
    { "source": "/global-leaderboard", "destination": "/_leaderboard/index.html" },
    { "source": "/global-leaderboard-demo", "destination": "/global-leaderboard-demo.html" },
    { "source": "/test-upload-simple", "destination": "/test-upload-simple.html" },
    { "source": "/document-upload", "destination": "/document-upload.html" },
    { "source": "/test-worker-url", "destination": "/test-worker-url.html" },
    { "source": "/encrypted-upload", "destination": "/encrypted-upload.html" },
    { "source": "/agencyonboarding", "destination": "/agencyonboarding.html" }
  ],
  "redirects": [
    { "source": "/public/:path*", "destination": "/:path*", "permanent": true },
    { "source": "/_admin", "destination": "/admin", "permanent": false },
    { "source": "/_admin/", "destination": "/admin", "permanent": false },
    { "source": "/_manager", "destination": "/manager", "permanent": false },
    { "source": "/_manager/", "destination": "/manager", "permanent": false },
    { "source": "/_agent", "destination": "/agent", "permanent": false },
    { "source": "/_agent/", "destination": "/agent", "permanent": false },
    { "source": "/_customer-service", "destination": "/customer-service", "permanent": false },
    { "source": "/_customer-service/", "destination": "/customer-service", "permanent": false },
    { "source": "/_super-admin", "destination": "/super-admin", "permanent": false },
    { "source": "/_super-admin/", "destination": "/super-admin", "permanent": false }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; base-uri 'self'; object-src 'none'; frame-ancestors 'none'; frame-src https://js.stripe.com https://hooks.stripe.com; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://cdn.tailwindcss.com https://cdnjs.cloudflare.com https://unpkg.com https://cdn.jsdelivr.net https://js.stripe.com https://maps.googleapis.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://fonts.gstatic.com https://cdn.jsdelivr.net https://cdnjs.cloudflare.com; font-src 'self' data: https://fonts.googleapis.com https://fonts.gstatic.com https://cdn.jsdelivr.net https://cdnjs.cloudflare.com; img-src 'self' data: https:; connect-src 'self' https://*.supabase.co https://api.openai.com https://unpkg.com https://cdn.jsdelivr.net https://api.stripe.com https://hooks.stripe.com https://*.workers.dev https://maps.googleapis.com https://fonts.googleapis.com https://fonts.gstatic.com; upgrade-insecure-requests;"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}